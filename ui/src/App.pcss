:root {
	--height: 80px;
}

.App {
  text-align: center;
}

.logo {
  animation: logoSpin infinite 20s linear;
  height: var(--height);
}

.header {
  background-color: #222;
  height: 150px;
  color: white;
}

.intro {
  font-size: large;
}

.thing { font-size: 200px;}
@keyframes logoSpin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}
